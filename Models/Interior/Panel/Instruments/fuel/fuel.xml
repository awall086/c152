    <ind>15</ind>
    <dep>25</dep>
   </entry>
   <entry>
    <ind>18.7</ind>
    <dep>38</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.000</x-m>
   <y-m>0.020</y-m>
   <z-m>-0.011</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


