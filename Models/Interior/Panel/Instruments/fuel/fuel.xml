<?xml version="1.0"?>

<PropertyList>

 <path>fuel.ac</path>

	<animation>
		<type>material</type>
		<object-name>Fuel_base</object-name>
		<object-name>LFuelNeedle</object-name>
		<object-name>RFuelNeedle</object-name>
		<emission>
			<red>0.98</red>
			<green>0.51</green>
			<blue>0.0</blue>
			<factor-prop>/sim/model/c152/lighting/instruments</factor-prop>
		</emission>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>LFuelNeedle</object-name>
		<property>consumables/fuel/tank[1]/level-gal_us_norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>-43</dep></entry>
			<entry><ind>3.7</ind><dep>-18</dep></entry>
			<entry><ind>7.35</ind><dep>8</dep></entry>
			<entry><ind>10.6</ind><dep>25</dep></entry>
			<entry><ind>13.0</ind><dep>38</dep></entry>
		</interpolation>
		<center>
			<x-m>0.000</x-m>
			<y-m>-0.020</y-m>
			<z-m>-0.011</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>RFuelNeedle</object-name>
		<property>consumables/fuel/tank[2]/level-gal_us_norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>-43</dep></entry>
			<entry><ind>3.7</ind><dep>-18</dep></entry>
			<entry><ind>7.35</ind><dep>8</dep></entry>
			<entry><ind>10.6</ind><dep>25</dep></entry>
			<entry><ind>13.0</ind><dep>38</dep></entry>
		</interpolation>
		<center>
			<x-m>0.000</x-m>
			<y-m>0.020</y-m>
			<z-m>-0.011</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>


