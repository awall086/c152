<?xml version="1.0"?>

<PropertyList>
    <path>highvolt.ac</path>

	<animation>
		<type>material</type> 
		<object-name>highvolt_light</object-name>
		<condition>		
			<and>
				<greater-than>		
					<property>/systems/electrical/volts</property>
					<value>0.0</value>
				</greater-than>
				<less-than>		
					<property>/systems/electrical/suppliers/alternator</property>
					<value>22.0</value>
				</less-than>
			</and>
		</condition>
		<emission>	
			<red>1</red>	
			<green>0</green>	
			<blue>0</blue>
		</emission>
	</animation>
	
</PropertyList>
