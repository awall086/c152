<?xml version="1.0"?>

<PropertyList>
    <path>Amp.ac</path>
    <animation> 
        <type>rotate</type>
        <object-name>AmpNeedle</object-name>
        <property>systems/electrical/amps</property>
        <interpolation>
            <entry><ind>-60</ind><dep>-51.55</dep></entry>
            <entry><ind>-30</ind><dep>-27.48</dep></entry>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>30</ind><dep>27.48</dep></entry>
            <entry><ind>60</ind><dep>51.55</dep></entry>
        </interpolation>
        <axis> 
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>  
        </center>
    </animation>        
</PropertyList>
